{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Sienna",
	"description":  "Sienna color with shades",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": ["#111111", "#ffffff"],
					"slug": "black-white",
					"name": "Black white"
				},
				{
					"colors": ["#111111", "#a0522d"],
					"slug": "black-accent",
					"name": "Black and Accent"
				},
				{
					"colors": ["#111111", "#e3b39b"],
					"slug": "black-light",
					"name": "Black and Light"
				},
				{
					"colors": ["#111111", "#64331c"],
					"slug": "black-dark",
					"name": "Black and Dark"
				},
				{
					"colors": [ "#a0522d", "#ffffff" ],
					"name": "Accent White",
					"slug": "accent-white"
				},
				{
					"colors": [ "#e3b39b", "#ffffff" ],
					"name": "Light White",
					"slug": "light-white"
				},
				{
					"colors": [ "#64331c", "#ffffff" ],
					"name": "Dark White",
					"slug": "dark-white"
				}
			],
			"gradients": [
				{
					"slug": "soft-dark-to-base",
					"gradient": "linear-gradient(to bottom, var(--wp--preset--color--dark) 0%, var(--wp--preset--color--base) 100%)",
					"name": "Vertical soft dark to base"
				},
				{
					"slug": "hard-dark-to-base",
					"gradient": "linear-gradient(to bottom, var(--wp--preset--color--dark) 50%, var(--wp--preset--color--base) 50%)",
					"name": "Vertical hard dark to base"
				}
			],
			"palette": [
				{
					"color": "#fafafa",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#a0522d",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#e3b39b",
					"name": "Light",
					"slug": "light"
				},
				{
					"color": "#64331c",
					"name": "Dark",
					"slug": "dark"
				}
			]
		}
	}
}